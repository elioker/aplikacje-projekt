{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "sessions",
  "apis": [
    {
      "path": "/login.json",
      "operations": [
        {
          "summary": "Gather a token",
          "parameters": [
            {
              "paramType": "form",
              "name": "session[email]",
              "type": "string",
              "description": "User email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "session[password]",
              "type": "string",
              "description": "User password",
              "required": true
            }
          ],
          "nickname": "Sessions#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/logout.json",
      "operations": [
        {
          "summary": "Invalidate a token",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication token",
              "required": true
            }
          ],
          "nickname": "Sessions#destroy",
          "method": "get"
        }
      ]
    }
  ],
  "authorizations": null
}